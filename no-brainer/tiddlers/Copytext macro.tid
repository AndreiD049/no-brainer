\define copy-text(text)
<$button class="tc-btn-invisible" message="tm-copy-to-clipboard"
param="$text$" style="text-align: justify">
<code><$text text="$text$"/> {{$:/core/images/copy-clipboard}}
</code>
</$button>
\end